		h1
			padding-top 160px
			
	.boardList1, .boardList2
		text-decoration none
		list-style-type none
		width 50%
		float left
		padding 0
		margin 0
		
	.boardLists
		margin 0 auto
		text-align center
		
		ul
			li
				padding 10px
				
			padding-bottom 50px	
			
	.newMembers
		padding-bottom 50px
		
	.board_2
		
		background $lightBlue
		h2
			padding-top 50px				
			

	.newMemberBlock
		padding-top 50px
		overflow hidden
		
		img
			max-width 100%
			height auto
			width 80%
		
		.newMemberLeft
			float left
			clear both
			
		.newMemberRight
			float left
			width 60%
			padding-top 3%
			


			

		
